{
  "name": "lord",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "yarn generate-types && yarn lint && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "tsc --noemit && eslint --fix './**/*.{js,ts,svelte}' && npx svelte-check --threshold error",
    "format": "prettier --plugin-search-dir . --write .",
    "generate-types": "ts-node devtools/graphql-codegen/CLI.ts --esm"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@types/cookie": "^0.5.1",
    "@types/d3": "^7.4.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "apollo": "^2.34.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.63.6",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-eslint-parser": "^0.32.1",
    "svelte-preprocess": "^5.0.4",
    "svelte-preprocess-sass": "^2.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "type": "module",
  "dependencies": {
    "@figliolia/enhanced-stores": "^1.0.2",
    "@figliolia/event-emitter": "^1.1.1",
    "@figliolia/task-queue": "^1.0.0",
    "d3": "^7.8.5",
    "moment": "^2.29.4",
    "pageswitch": "https://github.com/alexfigliolia/pw.git"
  }
}